{
  "name": "standard-release-notes",
  "version": "0.0.0",
  "description": "A GitHub action to extract only the notes of the latest release from the changelog generated by standard-version.",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "ncc build index.js --license licenses.txt"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yashanand1910/standard-release-notes.git"
  },
  "keywords": [
    "github",
    "action"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yashanand1910/standard-release-notes/issues"
  },
  "homepage": "https://github.com/yashanand1910/standard-release-notes#readme",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^4.0.0"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.24.1",
    "husky": "^4.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && git add dist/"
    }
  }
}
